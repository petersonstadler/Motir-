@model IEnumerable<frontend_sistema.Models.Patrocinador>

<h1>Todos Patrocinadores</h1>

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Nome</th>
                <th scope="col">Status</th>
                <th scope="col">Responsavel</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var patrocinador in Model)
            {
                <tr scope="row">
                    <td>@patrocinador.Nome</td>
                    <td><span class="badge bg-success small">@patrocinador.Status</span></td>
                    <td>@patrocinador.Responsavel</td>
                    <td><a class="btn btn-primary" asp-action="Alterar" asp-route-id="@patrocinador.Id">alterar</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>